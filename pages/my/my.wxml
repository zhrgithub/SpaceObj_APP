<view class="container">
		<!-- 未登录 -->
		<view class="login-background-style" bindtap="toLogin" wx:if="{{!loginStatus}}">
			<view class="photo-image-background-style">
				<image src="/static/photo.png" mode=""></image>
			</view>
			<view class="userinfo-background-style">
				<view class="btn-login-background">
					登录
				</view>
			</view>
		</view>
		<!-- 已登录 -->
		<view class="login-background-style" wx:if="{{loginStatus}}" bindtap="editUserInfo">
			<view class="photo-image-background-style">
				<image src="{{photoUrl}}" mode=""></image>
			</view>
			<view class="userinfo-background-style">
				<text class="nick-name-style">{{nickName}}</text>
				<text class="address-background-style">IP属地：{{ipTerritory}}</text>
			</view>
			<!-- 设置空白区域 -->
			<!-- <view class="userinfo-background-style" wx:if="online==0">
				
			</view> -->
			<view class="to-perso-certer">
				<image src="/static/toPersonCerter.png" mode=""></image>
			</view>
		</view>

		<!-- 实名认证 -->
		<view class="invite-value-background-style" wx:if="{{loginStatus&&online==1}}">
			<view class="link-us-style">
				实名状态：
				<text wx:if="{{realNameStatus==0}}">未实名</text>

				<text wx:if="{{realNameStatus==2}}">审核中</text>
				<text wx:if="{{realNameStatus==3}}">审核不通过</text>
			</view>


			<view class="invite-btn-stye" bindtap="userVerified" wx:if="{{realNameStatus!=1}}">
				去实名
			</view>

			<view class="real-name-finish" wx:if="{{realNameStatus==1}}">
				已实名
			</view>

		</view>

		<!-- 邀请链接 -->
		<view class="invite-value-background-style" wx:if="{{loginStatus&&online==1}}">
			<view class="invite-tips-style">
				邀请值：
			</view>
			<view class="invite-numbers-style">
				{{invitationValue}}
			</view>
			<view class="invite-btn-stye">
				<button data-name="inviteFriend" open-type="share">邀请好友</button>
			</view>
		</view>
		<view class="invite-hint-background" wx:if="{{loginStatus&&online==1}}">
			提示：邀请值可以用来获取用户联系方式，每获取一次用户联系方式，邀请值会减少 1 ，邀请好友注册一次邀请值会增加 1
		</view>

		<!-- 好友助力 -->
		<view class="invite-value-background-style" bindtap="friendHelp" wx:if="{{online==1}}">
			<view class="friend_help">
				我分享的项目
			</view>

			<view class="to-perso-certer">
				<image src="/static/toPersonCerter.png" mode=""></image>
			</view>
		</view>

		<!-- 我的发布 -->
		<view class="invite-value-background-style" bindtap="myRelease" wx:if="{{online==1}}">
			<view class="friend_help">
				我发布的项目
			</view>

			<view class="to-perso-certer">
				<image src="/static/toPersonCerter.png" mode=""></image>
			</view>
		</view>

		<!-- 联系客服 -->
		<view class="invite-value-background-style" wx:if="{{online==1}}">
			<view class="link-us-style">
				客服微信
			</view>

			<view class="invite-btn-stye" bindtap="copyWeChat">
				复制
			</view>
		</view>
		<!-- 下载 -->
		<view class="invite-value-background-style" wx:if="{{online==1}}">
			<view class="link-us-style">
				APP
			</view>

			<view class="invite-btn-stye" bindtap="downloadFunction">
				下载
			</view>
		</view>


		<view wx:if="{{userType=='root'&&loginStatus}}">
			<!-- 管理 -->
			<view class="invite-value-background-style">

				<view class="manage-btn-style" bindtap="auditProject">
					项目管理
				</view>

				<view class="manage-btn-style" bindtap="userManagement">
					用户管理
				</view>
				<view class="manage-btn-style" bindtap="userVerifiedMangeMent">
					实名管理
				</view>
				<view class="manage-btn-style" bindtap="photoManagement">
					头像管理
				</view>
			</view>

			<view class="invite-value-background-style">

				<view class="manage-btn-style" bindtap="advertiseManagement">
					广告管理
				</view>
				<view class="manage-btn-style" bindtap="logManagement">
					日志管理
				</view>
				<view class="manage-btn-style" bindtap="otherManagement">
					其它管理
				</view>
			</view>

			<!-- 其它管理 -->
			<view class="space-line-style">

			</view>
		</view>
	</view>